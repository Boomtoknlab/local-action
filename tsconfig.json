{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "lib": ["ES2022"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noEmit": true,
    "noImplicitAny": false,
    "paths": {},
    "resolveJsonModule": true,
    "rootDir": ".",
    "strict": false,
    "target": "ES2022"
  },
  "exclude": ["__fixtures__", "__tests__", "coverage", "node_modules"]
}
